{
  "name": "demo-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenv -e .env.development.local -- nest start",
    "start:dev": "dotenv -e .env.development.local -- nest start --watch",
    "start:debug": "dotenv -e .env.development.local -- nest start --debug --watch",
    "start:prod": "dotenv -e .env.development.local -- node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests  --config ./jest.config.js",
    "test:watch": "jest --config ./jest.config.js --passWithNoTests --watch ",
    "test:cov": "jest --config ./jest.config.js --passWithNoTests --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./jest.config.js --passWithNoTests  ",
    "test:e2e": "dotenv -e .env.development.local --  jest --passWithNoTests --detectOpenHandles --forceExit --no-cache --config ./jest.e2e.js",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "docker:up": "docker-compose --env-file .env.development up -d",
    "docker:down": "docker-compose --env-file .env.development down"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@prisma/client": "^5.17.0",
    "bcryptjs": "^2.4.3",
    "cache-manager-redis-yet": "^5.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.7.0",
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.11",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.8.0",
    "husky": "^9.1.3",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.7",
    "nyc": "^17.0.0",
    "prettier": "^3.0.0",
    "prisma": "^5.17.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1"
  },
  "lint-staged": {
    "*.ts": [
      "pnpm lint",
      "pnpm format"
    ]
  }
}
